struct floatYPR
{
    yaw : float;
    pitch : float;
    roll : float;
}

enum MotorId
{
    PROP = 1;
}

group control
{
	message Motor : 2
	{
        motorId : MotorId;
	    speed : int8;
    }
}
group telemetry
{
	message Telemetry : 3
	{
	    orientation : floatYPR;
    }
}
group image
{
	message Image : 4
	{
	    type : int8;
        image : list<byte>;
    }
}
group debug
{
	message Debug : 5
	{
	    msg : string;
    }
}

