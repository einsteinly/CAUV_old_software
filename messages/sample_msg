struct floatYPR
{
    yaw : float;
    pitch : float;
    roll : float;
}

group control
{
	message Motor : 2
	{
	    motorId : uint8;
	    speed : int8;
    }
}
group telemetry
{
	message Telemetry : 3
	{
	    orientation : floatYPR;
    }
}
group image
{
	message Image : 4
	{
	    type : int8;
        image : list<byte>;
    }
}
group debug
{
	message Debug : 5
	{
	    msg : string;
    }
}

